{
  "name": "image-buy-app",
  "version": "1.0.0",
  "description": "Professional event photo selling platform with watermark removal after payment",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && npm run dev",
    "frontend": "cd client && npm start",
    "backend:start": "cd backend && npm start",
    "frontend:start": "cd client && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../client && npm install",
    "setup-db": "cd backend && npm run setup-db",
    "build": "cd client && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "photo-selling",
    "event-photos",
    "watermark-removal",
    "mypos-payments",
    "professional-photography"
  ],
  "author": "",
  "license": "MIT"
}
